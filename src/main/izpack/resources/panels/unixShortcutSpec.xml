<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
  <programGroup defaultName="NeuBiro-@app.version@" location="applications"/>

  <shortcut
    name="NeuBiro"
    programGroup="yes"
    desktop="yes"
    applications="no"
    startMenu="no"
    startup="no"
    target="$INSTALL_PATH/bin/neubiro"
    commandLine="en"
    initialState="normal"
    iconFile="$INSTALL_PATH/resources/neubiro.png"
    iconIndex="0"
    workingDirectory="$INSTALL_PATH"
    type="Application"
    encoding="UTF-8"
    KdeSubstUID="true"
    KdeUsername="root"
    createForAll="false"
    Categories="Application;Development;"
    description="">
  </shortcut>

  <shortcut
    name="Uninstall NeuBiro"
    programGroup="yes"
    desktop="no"
    applications="no"
    startMenu="no"
    startup="no"
    target="/usr/bin/java"
    commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
    initialState="noShow"
    iconFile="trashcan_full"
    iconIndex="0"
    workingDirectory=""
    type="Application"
    encoding="UTF-8"
    KdeSubstUID="true"
    KdeUsername="root"
    createForAll="false"
    Categories="Application;Development;"
    description="This uninstalls NeuBiro @app.version@">
  </shortcut>
</shortcuts>
